{
  "name": "tappleby/laravel-asset-manifest",
  "description": "Laravel 4 package for re-mapping assets paths, useful for asset revving using tools such as grunt and/or loading assets from a CDN.",
  "authors": [
    {
      "name": "Terry Appleby",
      "email": "terry.appleby@gmail.com"
    }
  ],
  "keywords": ["assets", "cdn", "laravel"],
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "illuminate/support": "~4.1",
    "illuminate/filesystem": "~4.1",
    "illuminate/routing": "~4.1"

  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev",
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-0": {
      "Tappleby\\AssetManifest": "src/"
    }
  },
  "minimum-stability": "stable"
}
